<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <!--别名-->
        <servlet-name>login</servlet-name>
        <!--servlet所在的类-->
        <servlet-class>com.zpj.servlet.LoginServlet</servlet-class>

    </servlet>

    <servlet-mapping>
        <!--别名-->
        <servlet-name>login</servlet-name>
        <!--资源名-->
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>smsServlet</servlet-name>
        <servlet-class>com.zpj.servlet.SmsLoginServlet</servlet-class>

    </servlet>

    <servlet-mapping>
        <servlet-name>smsServlet</servlet-name>
        <url-pattern>/SmsLoginServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>registerServlet</servlet-name>
        <servlet-class>com.zpj.servlet.RegisterServlet</servlet-class>

    </servlet>

    <servlet-mapping>
        <servlet-name>registerServlet</servlet-name>
        <url-pattern>/RegisterServlet</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>AnalysisAllServlet</servlet-name>
        <servlet-class>com.zpj.servlet.AnalysisAllServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AnalysisAllServlet</servlet-name>
        <url-pattern>/AnalysisAllServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>AnalysisPartServlet</servlet-name>
        <servlet-class>com.zpj.servlet.AnalysisPartServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AnalysisPartServlet</servlet-name>
        <url-pattern>/AnalysisPartServlet</url-pattern>
    </servlet-mapping>
    <!--<welcome-file-list>-->
        <!--<welcome-file>../test.jsp</welcome-file>-->
    <!--</welcome-file-list>-->
</web-app>
